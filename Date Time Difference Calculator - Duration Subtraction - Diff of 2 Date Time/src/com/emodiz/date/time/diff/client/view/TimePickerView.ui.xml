<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:m="urn:import:gwt.material.design.client.ui"
	 xmlns:ma="urn:import:gwt.material.design.addins.client"
	>
	<m:MaterialPanel>
		 <ma:timepicker.MaterialTimePicker ui:field="timePicker" detectOrientation="true" autoClose="true"/>
		<m:MaterialButton ui:field="currentTime" text="Current"
			waves="LIGHT" textColor="WHITE"  />
		<m:MaterialButton ui:field="clearDate" text="Clear"
			waves="DEFAULT" backgroundColor="WHITE" textColor="BLACK" />
	</m:MaterialPanel>
</ui:UiBinder> 